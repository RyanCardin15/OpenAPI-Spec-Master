{
  "name": "openapi-visualizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "mcp:dev": "node --loader ts-node/esm src/mcp/server.ts",
